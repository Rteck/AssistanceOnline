
@{
    ViewBag.Title = "createAccount";
    Layout = "~/Views/Shared/_LoginAssistanceOnline.cshtml";
}

<h2>Personal information</h2>
<hr />
@Html.ValidationSummary(true, "", new { @class = "text-danger" })
@using (Html.BeginForm("Login", "Login", FormMethod.Post))
{
    <div class="form-group">
        <label for="exampleInputEmail1">Email address</label>
        <input type="email" class="form-control" id="exampleInputEmail1" placeholder="">
    </div>
        <div class="form-group">
            <label for="exampleInputPassword1">Password</label>
            <input type="password" class="form-control" id="exampleInputPassword1" placeholder="" required>
        </div>
        <div class="form-group">
            <input type="submit" class="btn btn-primary" id="login" placeholder="" style="width:100%">
        </div>
        <div class="form-group-lg">
            @Html.ActionLink("create acount", "createAccount")
            @Html.ActionLink("forgot password?", "forgotPassword")
        </div>

        <div class="form-group-lg">
            <div class="g-recaptcha" data-sitekey="6LfU5CsUAAAAAOXswZ81RSO4phwBuY3mP_DVheB-"></div>
        </div>

}

